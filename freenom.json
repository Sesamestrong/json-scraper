{
  "steps": [
    {
      "headers": {
        "uri": "https://my.freenom.com/clientarea.php",
        "jar": "freenom"
      },
      "frame":{
        "$token":"input[name=token] @ value"
      }
    },
    {
      "headers": {
        "uri": "https://my.freenom.com/dologin.php",
        "method": "POST",
        "jar": "freenom",
        "headers": {
          "content-type": "application/x-www-form-urlencoded",
          "Referer": "https://my.freenom.com/clientarea.php"
        },
        "body": {
          "username": "${username}",
          "password": "${password}",
          "token":"${token}"
        },
        "json": "true",
        "status-codes":[
          "302"
        ]
      }
    },
    {
      "headers": {
        "uri": "https://my.freenom.com/clientarea.php",
        "jar": "freenom",
        "headers":{
          "Referer":"https://my.freenom.com/clientarea.php"
        }
      },
      "frame": {
        "$hello": "h1.splash"
      }
    }
  ]
}
